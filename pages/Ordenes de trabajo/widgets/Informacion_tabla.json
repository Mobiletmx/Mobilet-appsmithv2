  "renderMode": "CANVAS",
  "responsiveBehavior": "fill",
  "rightColumn": 64,
  "searchKey": "",
  "serverSidePaginationEnabled": false,
  "tableData": "{{\n  (() => {\n    const ordenes = GetOrdenesTrabajo.data || [];\n    const pedidos = GetPedidos.data || [];\n\n    // Indexar GetPedidos por id_item_pedido para búsqueda rápida\n    const pedidosById = (pedidos || []).reduce((acc, p) => {\n      const key = String(p.id_item_pedido ?? \"\");\n      if (key) acc[key] = p;\n      return acc;\n    }, {});\n\n    return (ordenes || []).map(ot => {\n      const otKey = String(ot.id_item_pedido ?? ot._id_item_pedido ?? \"\");\n\n      const pedido = pedidosById[otKey] || {};\n\n      return {\n        // conservar todos los campos originales de la orden\n        ...ot,\n        // campo agregado con el estatus del pedido\n        pedido_estatus: pedido.estatus_pedido ?? null,\n        // opcional: objeto completo del pedido si lo necesitas más adelante\n        pedido_raw: pedido\n      };\n    });\n  })()\n}}",
