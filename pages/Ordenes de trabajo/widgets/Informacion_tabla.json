      "id": "customColumn1",
      "index": 8,
      "isCellEditable": false,
      "isCellVisible": true,
      "isDerived": true,
      "isDisabled": "{{(() => { const tableData = Informacion_tabla.processedTableData || []; return tableData.length > 0 ? tableData.map((currentRow, currentIndex) => (\n  (() => {\n    const act = currentRow?.actividad_programada;\n    const est = currentRow?.estatus_ot;\n    const enabled =\n      (act === \"Limpieza con cliente\" && est === \"Activa\") ||\n      (act === \"Limpieza en almacen\" && est === \"Activa\") ||\n      (act === \"Reparacion en almacen\" && est === \"Activa\") ||\n      (act === \"Reparacion en almacen\" && est === \"Completada\");\n    return !enabled;\n  })()\n)) : \n  (() => {\n    const act = currentRow?.actividad_programada;\n    const est = currentRow?.estatus_ot;\n    const enabled =\n      (act === \"Limpieza con cliente\" && est === \"Activa\") ||\n      (act === \"Limpieza en almacen\" && est === \"Activa\") ||\n      (act === \"Reparacion en almacen\" && est === \"Activa\") ||\n      (act === \"Reparacion en almacen\" && est === \"Completada\");\n    return !enabled;\n  })()\n })()}}",
      "isDiscardVisible": true,
      "isEditable": false,
      "isSaveVisible": true,
      "isVisible": true,
      "label": "Editar",
      "labelColor": "#FFFFFF",
      "notation": "standard",
      "onClick": "{{\n  (() => {\n    const row = Informacion_tabla.triggeredRow || Informacion_tabla.selectedRow;\n    if (!row) { showAlert(\"Selecciona un renglón para editar.\", \"warning\"); return; }\n\n    const act = row.actividad_programada;\n    const est = row.estatus_ot;\n    const enabled =\n      (act === \"Limpieza con cliente\" && est === \"Activa\") ||\n      (act === \"Limpieza en almacen\" && est === \"Activa\") ||\n      (act === \"Reparacion en almacen\" && est === \"Activa\") ||\n      (act === \"Reparacion en almacen\" && est === \"Completada\");\n    if (!enabled) { showAlert(\"No puedes editar esta orden.\", \"error\"); return; }\n\n    // Normalizar ID PK desde la fila (variantes posibles)\n    const pk = row.id_orden_de_trabajo ?? row._id_orden_de_trabajo ?? row.id_orden ?? row._id_orden ?? null;\n\n    // Normalizar y guardar stores: otSel, otForm (precarga) y otPending (payload)\n    const normalized = {\n      ...row,\n      id_orden_de_trabajo: pk,\n      _id_orden_de_trabajo: pk,\n      id_orden: pk\n    };\n\n    storeValue(\"otIsEditMode\", true, true);\n    storeValue(\"otSel\", normalized, true);\n    // precargar otForm con la fila normalizada (para mostrar en inputs)\n    storeValue(\"otForm\", normalized, true);\n    // asegurar que otPending contiene la PK y la fila para el payload\n    storeValue(\"otPending\", normalized, true);\n\n    // Si tienes helper que normaliza la fila, ejecútalo y luego reparcheamos PK\n    if (JS_OT?.setFromOrden) {\n      try {\n        JS_OT.setFromOrden(row);\n      } catch (e) {\n        console.warn(\"JS_OT.setFromOrden falló:\", e);\n      }\n      // Reparchear por si JS_OT sobreescribió stores y perdió la PK\n      const afterForm = appsmith.store.otForm || {};\n      const pkAfter = afterForm.id_orden_de_trabajo ?? afterForm._id_orden_de_trabajo ?? afterForm.id_orden ?? pk;\n      const reparche = { ...afterForm, id_orden_de_trabajo: pkAfter, _id_orden_de_trabajo: pkAfter, id_orden: pkAfter };\n      storeValue(\"otForm\", reparche, true);\n      storeValue(\"otPending\", reparche, true);\n      storeValue(\"otSel\", reparche, true);\n    }\n\n    showModal(\"Modal_Procesar\");\n  })()\n}}",
