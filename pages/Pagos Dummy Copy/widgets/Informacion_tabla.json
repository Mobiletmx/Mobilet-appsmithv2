  "serverSidePaginationEnabled": false,
  "tableData": "{{\n(() => {\n  const cobros = GetCobros.data || [];\n  const pedidos = GetPedidos.data || [];\n  const pagos = GetPagos.data || [];\n\n  // Indexar pedidos por id_item_pedido (como antes)\n  const pedidosById = (pedidos || []).reduce((acc, p) => {\n    const k = String(p.id_item_pedido ?? p._id_item_pedido ?? \"\").trim();\n    if (k) acc[k] = p;\n    return acc;\n  }, {});\n\n  // Indexar pagos por id_pago (intentar string y número)\n  const pagosByIdPagoStr = {};\n  const pagosByIdPagoNum = {};\n  (pagos || []).forEach(p => {\n    const raw = p.id_pago ?? p._id_pago ?? p.id_pago_personal ?? p.id ?? null;\n    const s = raw === null || raw === undefined ? \"\" : String(raw).trim();\n    if (s) pagosByIdPagoStr[s] = p;\n    const n = Number(raw);\n    if (!isNaN(n)) pagosByIdPagoNum[n] = p;\n  });\n\n  // Helper para obtener pago asociado a un cobro intentando varias claves\n  const findPagoForCobro = (c) => {\n    const candidates = [\n      c.id_cobro, c._id_cobro, c.id, c._id\n    ];\n    for (let cand of candidates) {\n      if (cand === undefined || cand === null) continue;\n      const s = String(cand).trim();\n      if (s && pagosByIdPagoStr[s]) return pagosByIdPagoStr[s];\n      const n = Number(cand);\n      if (!isNaN(n) && pagosByIdPagoNum[n]) return pagosByIdPagoNum[n];\n    }\n    return null;\n  };\n\n  return (cobros || []).map(c => {\n    const pedidoKey = String(c.id_item_pedido ?? c._id_item_pedido ?? \"\").trim();\n    const pedido = pedidosById[pedidoKey] || {};\n\n    const estatus_pedido_from_pedidos = pedido.estatus_pedido ?? null;\n    const estatus_pago_from_pedidos = !!pedido.estatus_pago;\n\n    // buscar el pago (1:1) por id_pago === id_cobro (intenta string y number)\n    const pago = findPagoForCobro(c);\n\n    const id_aplicacion_pago = pago ? (pago.id_aplicacion_pago ?? null) : null;\n    const monto_total_pagado = pago ? (Number(pago.monto_total_pagado ?? pago.monto_total ?? pago.monto ?? 0) || 0) : 0;\n\n\t\t\n    return {\n      ...c,\n      estatus_pedido_from_pedidos,\n      estatus_pago_from_pedidos,\n      id_aplicacion_pago,\n      monto_total_pagado: Number(Math.round((monto_total_pagado + Number.EPSILON) * 100) / 100)\n    };\n  });\n})();\n}}",
