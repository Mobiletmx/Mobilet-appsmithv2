  "serverSidePaginationEnabled": false,
  "tableData": "{{\n(() => {\n  const cobros = GetCobros.data || [];\n  const pedidos = GetPedidos.data || [];\n\n  // Crear índice rápido de pedidos por id_item_pedido\n  const pedidosById = (pedidos || []).reduce((acc, p) => {\n    const k = String(p.id_item_pedido ?? p._id_item_pedido ?? \"\");\n    if (k) acc[k] = p;\n    return acc;\n  }, {});\n\n  // Filtrar solo los cobros pendientes (estatus_pago = false)\n  const cobrosPendientes = cobros.filter(c => c.estatus_pago === true);\n\n  // Enriquecer con datos del pedido\n  return (cobrosPendientes || []).map(c => {\n    const key = String(c.id_item_pedido ?? c._id_item_pedido ?? \"\");\n    const pedido = pedidosById[key] || {};\n\n    const estatus_pedido_from_pedidos =\n      pedido.estatus_pedido ?? pedido.estatus ?? pedido.status ?? null;\n\n    return {\n      ...c,\n      estatus_pedido_from_pedidos,\n      // no incluimos pedido_raw para evitar traer objetos grandes\n      pedido_min: {\n        id_item_pedido: pedido.id_item_pedido ?? pedido._id_item_pedido ?? null,\n        id_cliente: pedido.id_cliente ?? null,\n        estatus_pedido: estatus_pedido_from_pedidos,\n      },\n    };\n  });\n})()\n}}",
